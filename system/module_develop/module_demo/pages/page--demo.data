pages
- page_demo
    title: Demonstration
    url
      match: %^/demo$%
    access
      roles
      - anonymous: anonymous
      - logged: logged
    content
    - demo_text
        type: text
        content: [demo_text] Some inline text.
        region: content
    - demo_code
        type: code
        handler: \effectivecore\modules\demo\events_page::on_show_code
        region: content
    - demo_text_from_file
        type: file
        file: include/text.txt
        region: content
    - demo_markup|markup
        type: span
        properties
          class: test-span
          style: font-weight: bold
        content
        - some_link|markup
            type: a
            properties
              class: some-link
              href: /demo
            content: [demo_markup] some markup
        region: content
    - demo_link_to_object
        type: link
        entity: forms/demo/form_demo
        region: content