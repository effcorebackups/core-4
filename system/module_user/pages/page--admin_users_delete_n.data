pages
- page_admin_users_delete_n
    title: Delete user "%%_user_email"?
    url
      match: %^/admin/users/delete/[0-9]+$%
      args
      - user_id: 4
    access
      roles
      - admins: admins
    content
    - code_user_n_delete
        type: code
        handler: \effectivecore\modules\user\events_page::on_show_admin_users_delete_n
        region: content
    - form_user_n_delete
        type: link
        entity: forms/user/form_user_n_delete
        use_page_args: 1
        region: content