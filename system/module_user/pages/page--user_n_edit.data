pages
- page_user_n_edit
    title: %%_profile_edit_title
    url
      match: %^/user/[0-9]+/edit$%
      args
      - user_id: 2
    access
      roles
      - admins: admins
      - logged: logged
    content
    - code_on_page_user_n_edit
        type: code
        handler: \effectivecore\modules\user\events_page::on_show_user_n_edit
        region: content
    - form_user_n_edit
        type: link
        entity: forms/user/form_user_n_edit
        region: content