forms
- form_user_n_delete|form
    id: user_n_delete
    attributes
      method: post
      class: form-user
    on_submit
      handler: \effectivecore\modules\user\events_form::on_submit_user_n_delete
    children
    - button_delete|markup
        type: input
        attributes
          type: submit
          name: button
          value: delete
    - button_cancel|markup
        type: input
        attributes
          type: submit
          name: button
          value: cancel