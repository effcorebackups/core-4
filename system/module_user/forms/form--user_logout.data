forms
- form_user_logout|form
    attributes
      id: user_logout
      method: post
      class: form-user
    on_submit
      handler: \effectivecore\modules\user\events_form_factory::on_submit_user_logout
    children
    - button_logout|markup
        type: button
        attributes
          type: submit
          name: button
          value: logout
        children
        - label|text
            text: logout