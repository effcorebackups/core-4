# ─────────────────────────────────────────────────────────────────────
# page types
# ─────────────────────────────────────────────────────────────────────
- page__embedded
    name: page__embedded
    type: markup
    data|Node
      children
      - doctype|Markup_simple
          tag_name: !DOCTYPE
          attributes
          - html: true
      - html|Markup
          tag_name: html
          attributes
          - lang: %%_lang
          - dir: %%_dir
          - custom_attributes|Text_RAW
              text: %%_html_custom_attributes
          children
          - head|Markup
              tag_name: head
              children
              - meta|Text_RAW
                  text: %%_meta
              - meta_custom_global|Text_RAW
                  text: %%_meta_custom_global
              - meta_custom|Text_RAW
                  text: %%_meta_custom
              - title|Markup
                  tag_name: title
                  children
                  - content: %%_title
              - icons|Text_RAW
                  text: %%_icons
              - styles|Text_RAW
                  text: %%_styles
              - scripts|Text_RAW
                  text: %%_scripts
          - body|Markup
              tag_name: body
              attributes
              - custom_attributes|Text_RAW
                  text: %%_body_custom_attributes
              children
              - content: %%_body
