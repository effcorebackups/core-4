# ─────────────────────────────────────────────────────────────────────
# basic types
# ─────────────────────────────────────────────────────────────────────
- text__embedded
    name: text__embedded
    type: markup
    data|Markup
      tag_name: x-text
      attributes
      - data-id: %%_id
      - custom_attributes|Text_RAW
          text: %%_attributes
      children
      - content|Text_RAW
          text: %%_text
    is_ready_for_selection: true
- picture__embedded
    name: picture__embedded
    type: markup
    data|Markup_simple
      tag_name: img
      attributes
      - data-id: %%_id
      - src: %%_src
      - custom_attributes|Text_RAW
          text: %%_attributes
    is_ready_for_selection: true
- picture_in_link__embedded
    name: picture_in_link__embedded
    type: markup
    data|Markup
      tag_name: a
      attributes
      - data-id: picture-%%_id
      - href: %%_url
      - custom_attributes|Text_RAW
          text: %%_link_attributes
      children
      - picture|Markup_simple
          tag_name: img
          attributes
          - data-id: %%_id
          - src: %%_src
          - custom_attributes|Text_RAW
              text: %%_attributes
    is_ready_for_selection: true
- picture_or_picture_in_link__embedded
    name: picture_or_picture_in_link__embedded
